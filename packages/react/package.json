{
  "name": "whimsical-react",
  "version": "0.0.1-alpha.0",
  "main": "dist/WReact.umd.js",
  "module": "dist/WReact.es.js",
  "types": "dist/types/src/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/WReact.es.js",
      "require": "./dist/WReact.umd.js"
    }
  },
  "scripts": {
    "example": "vite dev",
    "dev": "npx concurrently -c \"bgBlue.bold,bgMagenta.bold\" -p \"{time}-{pid}-{name}\" -t \"HH:mm:ss\" --handle-input \"tsc -w\" \"cross-env NODE_ENV=development vite build --mode dev --watch\"",
    "build": "rimraf -rf dist && tsc && vite build && pnpm build:editor && pnpm build:engine",
    "build:engine": "vite build --mode engine",
    "build:editor": "vite build --mode editor"
  },
  "dependencies": {
    "axios": "0.27.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "whimsical-shared": "workspace:*"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "whimsical-shared": "workspace:*"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "2.1.0",
    "rimraf": "3.0.2",
    "vite": "3.1.0"
  }
}
