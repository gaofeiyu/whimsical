{
  "name": "whimsical-vsc-extension",
  "displayName": "whimsical",
  "description": "",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Snippets",
    "Other"
  ],
  "activationEvents": [
    "onCommand:whim.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "whim.helloWorld",
        "title": "Hello World"
      }
    ],
    "languages": [
      {
        "id": "whim",
        "aliases": [
          "Whim",
          "Whimsical",
          "whimsical",
          "whim"
        ]
      }
    ],
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/javascript.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@vscode/test-electron": "^2.2.0",
    "glob": "^8.0.3"
  }
}
