{
  "name": "whimsical-monorepo",
  "private": true,
  "version": "0.0.1",
  "keywords": [
    "frontend",
    "low-code"
  ],
  "description": "A low code editor across technology stacks",
  "main": "index.js",
  "scripts": {
    "build": "lerna run build",
    "clean": "lerna clean --yes",
    "dev": "lerna exec --scope whimsical -- pnpm dev",
    "dev:shared": "lerna exec --scope whimsical-shared -- pnpm dev",
    "publish:canary": "lerna publish --canary --yes",
    "publish:release": "lerna publish --yes",
    "test": "lerna exec --scope whimsical -- pnpm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gaofeiyu/whimsical.git"
  },
  "author": "feiyu.gao@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gaofeiyu/whimsical/issues"
  },
  "homepage": "https://github.com/gaofeiyu/whimsical#readme",
  "packageManager": "pnpm@7.12.2",
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/node": "^17.0.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "@vitejs/plugin-react": "^2.1.0",
    "@vitest/coverage-c8": "^0.23.4",
    "@vitest/ui": "^0.23.4",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "happy-dom": "^6.0.4",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "lerna": "^5.3.0",
    "lint-staged": "^12.5.0",
    "nx": "^14.7.17",
    "pnpm": "^7.12.2",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwind": "^2.2.12",
    "prettier-stylelint": "^0.4.2",
