    "stylelint-config-tailwindcss": "^0.0.6",
    "stylelint-order": "^5.0.0",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.6.4",
    "vite": "^3.1.3",
    "vitepress": "1.0.0-alpha.17",
    "vitest": "^0.23.4"
  },
  "pnpm": {
    "overrides": {
      "whimsical-shared": "workspace:*",
      "whimsical-react": "workspace:*"
    }
  }
